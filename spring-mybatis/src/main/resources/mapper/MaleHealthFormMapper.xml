<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.summer.tech.spring.mybatis.mapper.MaleHealthFormMapper">
  <select id="findMaleHealthByStudentId" parameterType="int" resultType="com.summer.tech.spring.mybatis.entity.MaleHealthForm">
  select id, student_id as studentId, prostate from male_health_form where student_id = #{id}
  </select>
</mapper>